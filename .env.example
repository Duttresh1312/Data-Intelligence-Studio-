# LLM Settings
# Applies to domain inference, dataset summary synthesis, intent parsing, and plan generation.
USE_LLM=false
LLM_PROVIDER=groq
GROQ_API_KEY=your_groq_api_key_here
LLM_BASE_URL=https://api.groq.com/openai/v1
LLM_MODEL=llama-3.1-8b-instant
LLM_TEMPERATURE=0.2
